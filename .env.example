# Supabase Configuration (Get from supabase.com/dashboard)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# OpenAI Configuration (Get from platform.openai.com/api-keys)
# NEVER commit your actual API key to GitHub!
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_SITE_NAME=Navya Sree Yellina Portfolio

# Contact Form Configuration
CONTACT_EMAIL_TO=your_email@gmail.com
RATE_LIMIT_PER_HOUR=100

# Email Service Configuration (Choose one)
# Option 1: Resend (Recommended - get free API key at resend.com)
RESEND_API_KEY=

# Option 2: Gmail with App Password
# EMAIL_SERVICE=gmail
# EMAIL_USER=your-email@gmail.com
# EMAIL_PASS=your-app-specific-password

# Option 3: Custom SMTP
# SMTP_HOST=smtp.gmail.com
# SMTP_PORT=587
# SMTP_SECURE=false
# EMAIL_USER=your-email@gmail.com
# EMAIL_PASS=your-password

# Analytics (Optional)
NEXT_PUBLIC_GA_ID=
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=